total_grid_points :: 100

global double q
double dx = 1/total_grid_points.1
double dt = 0.1*dx
double dt_dx = dt / dx

diff = fun(q) (q[-1] - q[0]) * dt_dx

init
  q = if i1> 20 then 0.0 else 1.0
end

step 5:total_grid_points.1
  k1 = diff(q)
  k2 = diff(q + diff(q)/2)
  k3 = diff(q + diff(q + diff(q)/2)/2)
  k4 = diff(q + diff(q + diff(q + diff(q)/2)/2))
  q = q + (k1 / 6 + k2 / 3 + k3 / 3 + k4 / 6)
end

step 0:5
  q = 1.0
end